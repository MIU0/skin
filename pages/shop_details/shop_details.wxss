.book-title {
  width: 100%;
  font-size: 30rpx;
  color: #383838;
  line-height: 50rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
   overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
   word-break: break-all;
  -webkit-line-clamp:1;
  -webkit-box-orient: vertical;
}

.book-script {
  font-size: 28rpx;
  color: #999;
  padding: 0 0 10rpx 10rpx;
  box-sizing: border-box;
}

.book-price {
  width: 100%;
  overflow: hidden;
  margin-top: 14rpx;
  padding-top: 10rpx;
}

.book-price .book-price-left {
  float: left;
  padding-left: 10rpx;
  box-sizing: border-box;
}

.book-price .book-price-left view:first-child {
  font-size: 30rpx;
  font-weight: 350;
  color: #e24d51;
}

