  font-size: 28rpx;
  text-decoration: line-through;
  color: #ccc;
}

.book-price .book-price-right {
  float: right;
  font-size: 24rpx;
  color: #fd3939;
  font-weight: 350;
  padding: 4rpx 10rpx;
  box-sizing: border-box;
  border: 2rpx solid #fd3939;
  border-radius: 6rpx;
  margin-right: 20rpx;
  /* overflow: ; */
}

.book-price .book-price-right image {
  width: 27rpx;
  height: 25rpx;
  display: inline-block;
  flex-direction: row;
  vertical-align: middle;
  margin-right: 10rpx;
}

.book-price .book-price-right text {
  display: inline-block;
  flex-direction: row;
  vertical-align: middle;
}

.book-message {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 15rpx 20rpx;
