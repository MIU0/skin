page{
  background:#f9f9f9; 
  padding-top: 13rpx;
  box-sizing: border-box
}
.all_store{
  background: #fefefe;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.all_store_list{
width: 96%;
display: flex;
  flex-direction: row; 
  border-bottom: 1px solid #eeeeee;
}
.store_list_left{
 width: 25%;
 height: 100%;
display: flex;
align-items: center;
margin-right: 10rpx;

}
.store_list_left image{
width: 80%;
/* margin: 0 auto; */
border-radius: 6rpx
}
.store_list_right{
 width: 73%;
 height: 100%;
}
.store_name{
  width: 100%;
  margin-top: 20rpx;
  color: #383838;
  font-size: 28rpx;
  overflow: hidden; 
   text-overflow: ellipsis; 
   display: -webkit-box;
   word-break: break-all; 
   -webkit-line-clamp:2;
   -webkit-box-orient: vertical; 
}
.store_bottom{
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
}
.store_bottom_price{
  color: #ff484e;
}
.store_bottom_img{
  width: 60rpx;
  height: 60rpx;
}
.store_bottom_img image{
  width: 100%;
  height: 100%;
}
.no_more{
  margin-top: 10rpx;
  width: 100%;
text-align: center;
height: 60rpx;
line-height: 60rpx;
color: #c2c2c2;
font-size: 26rpx;
}

.store_pop{
      width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 990;
    opacity: 0.7;
}
.store_pop_body{
 position: fixed;
  width: 100%;
  height: 308rpx;
  left: 0;
  bottom: 100rpx;
  z-index: 999;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background: #fff;
}
.store_pop_bottom{
 position: fixed;
  width: 100%;
  height: 100rpx;
  left: 0;
  bottom: 0;
  z-index: 999;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}
.store_pop_bottom view{
width: 50%;
height: 100rpx;
line-height: 100rpx;
text-align: center;
color: #fff;
font-size: 30rpx
}
.store_pop_bottom view:first-child{
  background: #555555;
}
.store_pop_bottom view:last-child{
  background: #fd4547;
}
.store_pop_top{
  width: 92%;
  height: 200rpx;
  /* border: 1px solid red; */
  position: relative;
  display: flex;
  flex-direction: row;
}
.store_pop_img{
  /* border: 1px solid red; */
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  bottom: 11px;
  left: 0;
  background: #f8f8f8;
  display: flex;
  align-items: center;
}
.store_pop_con{
  width: 60%;
  height: 100%;
  margin-left: 230rpx;
}
.store_pop_close{
 width: 50rpx;
 height: 50rpx;
 margin-top: 18rpx;
}
.store_pop_img image{
  width: 100%;
  height: 40%;

}
.store_pop_con_name{
  width: 99%;
  font-size: 28rpx;
  margin-top:16rpx; 
/* border: 1px solid red; */
  /*超出一行文字自动隐藏*/
  overflow:hidden; 
  /*文字隐藏后添加省略号*/
text-overflow:ellipsis;
/*强制不换行*/
white-space:nowrap; 
}
.store_pop_con_price{
  color: #e23c48;
  font-size: 28rpx;
}
.store_pop_close image{
  width: 83%;
  height: 80%;
}
.store_pop_num{
width: 92%;
height: 120rpx;
/* border: 1px solid red; */
display: flex;
flex-direction: row;
}
.store_pop_num_left{
width: 29%;
height: 100%;


}
.store_pop_num_left view:first-child{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
}
.store_pop_num_left view:last-child{
  font-size: 26rpx;
}
.store_pop_num_right{
  width: 71%;
height: 100%;
}
.one_top{
  /* width: 100%; */
  height: 50rpx;
  /* border: 1px solid red; */
  /* display: inline-block; */
  float: right;
  margin-top: 20rpx;
  margin-right: 10rpx;
}

 .one_top .add, .one_top .subtract {
    float: left;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 40rpx;
  border: 1px solid #e8e8e8;
  box-sizing: border-box;
  background: #fefefe;
  color: #6e6e6e;
}

.disabled{
color: #ddd;
}
.count{
   float: left;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-top: 1px solid #e8e8e8;
  border-bottom: 1px solid #e8e8e8;
  text-align: center;
  box-sizing: border-box;
  min-height: 1rem;
  font-size: 28rpx;
  color: #6e6e6e;
  background: #fefefe;
}
