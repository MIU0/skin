page{
  background-color: #f8f8f8;
}
.main{
  background-color: white;
  font-size: 28rpx;
  padding:30rpx 30rpx 0 30rpx;
  box-sizing: border-box;  
}
.head{
  margin-bottom: 30rpx;
}
.head text:nth-child(2){
  display: block;
  color: #999;
  font-size: 24rpx;
  margin-top:10rpx;
}
.list >view{
  overflow: hidden;
  border-bottom: 2rpx solid #f2f2f2;
  padding: 20rpx 0;
 
}
.list view image{
  width: 15rpx;
  height: 26rpx;
  float: right;
  margin-top: 8rpx;
  opacity: 0.5;
}