.swiper-tab {
  width:100%;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.swiper-tab-list {
  font-size: 24rpx;
  width: 99%;
  color: #555;
  border-bottom: 1rpx solid #eeeeee;
  border-right: 1rpx solid #eeeeee;
  text-align: center;
  min-height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.swiper-tab-list view{
width: 80%;
line-height: 40rpx;
}
.showc{
  border-left: 10rpx solid red;
}

