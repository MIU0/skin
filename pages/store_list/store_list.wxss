  font-size: 28rpx;
}
.code_info_body image{
width:350rpx;
height: 350rpx;
}
.code_info_hint{
   position: fixed;
   z-index: 999;
   top:70vh;
    margin: auto;
  left: 0;
  right: 0;
width: 100%;
height: 100rpx;
color: #fff;
font-size: 28rpx;
line-height: 100rpx;
text-align: center;
}
.zhuan_info_con_left button{
padding:0 !important;
margin:0;
background: none;
display: flex;
align-items: center;
justify-content: center;
}
.zhuan_info_con_left button::after{
  border: none
}



