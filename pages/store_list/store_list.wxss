.shop_item{
  width:100%;
  display: flex;
  flex-direction: row;  
  justify-content: flex-start;
   margin-top: 20rpx;
   padding: 10rpx 0;
   border-bottom: 1rpx solid #eeeeee;
}
.shop_item_top{
  width:30%;
  margin-right:20rpx;
  border-radius: 10rpx; 
}
.shop_item_bottom{
  text-align: left;
  line-height: 40rpx;
  color: #555;
  height: 100%;
  /* border: 1px solid red; */
}
.shop_item_bottom view:first-child{
  font-size: 28rpx;
  color: #494949;
  overflow: hidden; 
   text-overflow: ellipsis; 
   display: -webkit-box;
   word-break: break-all; 
   -webkit-line-clamp:1;
   -webkit-box-orient: vertical;
}
.shop_intro{
  font-size: 20rpx;
  margin-top: 10rpx;
 overflow: hidden; 
   text-overflow: ellipsis; 
   display: -webkit-box;
   word-break: break-all; 
   -webkit-line-clamp:1;
   -webkit-box-orient: vertical;
}
.shop_item_bottom view:last-child{
  font-size: 28rpx;
  margin-top: 30rpx;
  color: #df6b18 !important;
}
/*赚弹出*/
.zhuan{
  width: 80rpx;
  height: 80rpx;
  background:#f27c12;
  color: #fff;
  border-radius: 50%;
  text-align: center;
  line-height: 80rpx; 
  position: fixed;
  bottom: 10vh;
  right: 37rpx;
  font-size: 42rpx;
  border: 3px solid #fbfbfb;
}.zhuan_info{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 990;
  opacity: 0.7;
}
.zhuan_info_body {
  position: fixed;
  width: 100%;
  height: 43vh;
  bottom: 0;
  z-index: 999;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background: #fff;
  /* border-bottom: 1px solid #ebebeb; */
  font-size: 28rpx;
}
.zhuan_info_body_top{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx !important;
}
.zhuan_info_body_hint{
  width: 100%;
  line-height: 43rpx;
  padding:0 8%;
  box-sizing: border-box;
  text-align: center;
  color: #939393;
}
.zhuan_info_con{
  width: 100%;
  height: 160rpx;
   padding:0 8%;
  box-sizing: border-box;
  display: flex;
  margin-top: 10px;
   color: #939393;
    margin-bottom: 12px;

}
.zhuan_info_con_left{
  width:50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center; 
}
.zhuan_img{
  width: 100rpx;
  height: 100rpx;
}
.zhuan_btn{
   width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx !important;
  border: 1px solid #ebebeb;
  color: #313131;
}
.code_info{
 width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 990;
  opacity: 0.7;
}
.code_info_body{
   position: fixed;
  width: 75%;
  height: 45vh;
  margin: auto;
  left: 0;
  top:25vh;
  right: 0;
  z-index: 999;
  display: flex;
  align-items: center;
justify-content: center;
  background: #fff;
