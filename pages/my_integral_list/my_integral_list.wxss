/* pages/my_integral_list/my_integral_list.wxss */

page{
  background: #f8f8f8;
}
.more_data{
  text-align: center;
  font-size: 26rpx;
  color: #696767;
  margin-top: 15rpx;
}
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
.integral_bottom{
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.integral_bottom_null{
  width: 100%;
  height: 200rpx;
  line-height: 200rpx;
  text-align: center;
  font-size: 30rpx;
  color: #b1b1b1;
}
.integral_list{
  width:94%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
 padding: 10rpx 0;
  margin-bottom: 10rpx;
  color: #5e5d5d;
  border-bottom: 1px solid #ddd;
}
.integral_list_left{
  display: flex;
  flex-direction: column;
}
.integral_source_name{
  margin-bottom: 10rpx;
  font-size: 28rpx;
}
.integral_create_time{
  font-size: 26rpx;
}
.integral_list_right{
  font-size: 28rpx;
  color: #fe6505
}